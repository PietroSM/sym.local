{% extends 'base.html.twig' %}
{% block title %}
	{{ parent() }}!! Prueba2
{% endblock %}
{% block body %}
	{% if nombre is defined %}
		<p>Hola
			{{ nombre }}!!</p>
		<p>{{ saludo }}!!</p>
		<p>Hoy es
			{{ fecha | date("d D/M/y") }}</p>
	{% else %}
		<p>Hola mundo!!</p>
	{% endif %}
	<ul>
		{% for nombre in nombres %}
			<li>{{ nombre|upper }}</li>
		{% endfor %}
	</ul>
{% endblock %}
